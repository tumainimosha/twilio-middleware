{
  "name": "princeton255/twilio-middleware",
  "description": "A Laravel Middleware for Twilio API signature validation",
  "license": "MIT",
  "keywords": [
    "twilio",
    "middleware"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Tumaini Mosha",
      "email": "princeton.mosha@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "~5.7.0",
    "twilio/sdk": "^5.22"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Princeton255\\TwilioMiddleware\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Princeton255\\TwilioMiddleware\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
